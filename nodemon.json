{
  "restartable": "rs",
  "watch": [
    "src",
    ".env"
  ],
  "ext": "js,ts,json",
  "ignore": [
    "node_modules/**/node_modules",
    "src/**/*.spec.ts"
  ],
  "delay": "2500",
  "env": {
    "NODE_ENV": "development",
    "PORT": 4000
  },
  "exec": "ts-node  --transpile-only ./src/app.ts"
}